{
  "name": "dynamate-design-to-code",
  "version": "1.0.0",
  "description": "CLI tool for converting Figma designs to code with Cursor integration",
  "main": "dist/cli/index.js",
  "bin": {
    "dynamate-design-to-code": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "dotenv": "^16.3.1",
    "figma-js": "^1.16.0",
    "inquirer": "^8.2.6",
    "openai": "^4.20.1",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.6",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": ["<rootDir>/dist/"],
    "clearMocks": true,
    "resetMocks": true,
    "restoreMocks": true
  }
}
